{
  "name": "@jashkasoft/rx-jstore",
  "version": "0.0.1",
  "description": "JashkaStore - simple, flexible, reactive store",
  "main": "bundle/bundle.js",
  "types": "bundle/bundle.d.ts",
  "scripts": {
    "serve": "./node_modules/.bin/webpack-dev-server --config webpack.config-dev.js --mode development --profile --open --port 8000 --inline --hot --progress --profile --colors --watch --content-base examples/",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production webpack --mode production",
    "publish": "./create-bundle.sh && npm publish ./bundle --access public",
    "lib": "yarn run build && yarn pack ./bundle"
  },
  "keywords": [
    "store",
    "jashkasoft",
    "rxjs",
    "reactive store"
  ],
  "author": {
    "name": "JashkaSoft",
    "email": "jashkasoft@gmail.com",
    "url": "https://github.com/Insidexa"
  },,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Insidexa/jstore"
  },
  "homepage": "https://github.com/Insidexa/jstore",
  "dependencies": {
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@types/node": "7.0.0",
    "awesome-typescript-loader": "^5.0.0",
    "cross-env": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.1.6",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "typescript": "^2.7.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
